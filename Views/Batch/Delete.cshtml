@model SchoolMangement.Models.batch

@{
    ViewBag.Title = "Delete";
}

    <h2 class="text-capitalize text-primary text-center" style="margin-top:30px"> Delete Course </h2>


    <div class="container">
        <h3>Are you sure you want to delete this?</h3>
      
        <hr />
        <dl class="dl-horizontal">
            <dt>
                <label style="margin-bottom:10px; margin-left:0px;font-weight:bolder">Batch Name</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.batch1)
            </dd>

            <dt>
                <label style="margin-bottom:10px; margin-left:0px;font-weight:bolder">Year</label>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.year)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-danger" /> |
                @Html.ActionLink("Back to List", "Index")
            </div>
        }
    </div>
